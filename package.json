{
  "name": "autopilot",
  "version": "0.0.1",
  "description": "Fast app scaffolding and layout system, powered by CoPilotâ„¢",
  "repository": {
    "type": "git",
    "url": "git@github.com:sandro-pasquali/protopilot.git"
  },
  "keywords": [
    "gulp",
    "browserify",
    "backbone",
    "framework",
    "bundling",
    "build",
    "deploy"
  ],
  "scripts": {
    "preinstall": "npm list gulp -g || npm install gulp -g;",
    "postinstall": "gulp",
    "clean": "rm -rf node_modules; rm -rf build; rm -rf .sass-cache; rm -rf sass; rm -rf stylesheets; rm -rf test/views; rm -rf src/.traceur; rm -rf gulp/tasks/cache",
    "build": "node --harmony ./bin/build.js",
    "test": ""
  },
  "browser": {
    "auto": "./gulp/browserScripts/autopilot.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "auto": {
      "exports": "auto",
      "depends": [
        "backbone:Backbone",
        "underscore:_",
        "jquery:$",
        "bluebird:Promise"
      ]
    }
  },
  "devDependencies": {
    "browser-sync": "~1.3.0",
    "browserify": "~4.2.3",
    "browserify-shim": "~3.6.0",
    "chai": "^1.9.1",
    "compass": "*",
    "exorcist": "*",
    "gulp": "*",
    "gulp-beautify": "^1.1.0",
    "gulp-changed": "~0.4.0",
    "gulp-compass": "~1.1.9",
    "gulp-imagemin": "~0.1.5",
    "gulp-jshint": "^1.8.4",
    "gulp-mocha": "^1.0.0",
    "gulp-mocha-phantomjs": "^0.5.0",
    "gulp-notify": "^1.4.0",
    "gulp-open": "~0.2.8",
    "gulp-replace": "^0.4.0",
    "gulp-sourcemaps": "^1.1.4",
    "gulp-traceur": "^0.13.0",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "~3.0.0",
    "hbsfy": "~1.3.2",
    "mkdirp": "^0.5.0",
    "mocha": "^1.21.4",
    "products": "^3.0.0",
    "review": "^0.9.0",
    "rimraf": "*",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~0.10.1"
  },
  "dependencies": {
    "async": "*",
    "backbone": "*",
    "bluebird": "*",
    "bootstrap": "^3.2.0",
    "connect": "*",
    "jquery": "*",
    "underscore": "*"
  }
}
